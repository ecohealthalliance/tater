template(name="documentDetail")
  .document-detail-container
    .coding-container
      +codingKeywords
    .document-container
      #documentContainer
        .document-heading
          h1.dacoument-title= document.title
        #documentBody.document-body
          +paragraphText text=document.body
    .annotation-container
      each annotations
        p {{startIndex}} - {{endIndex}} : {{header}}, {{subHeader}}, {{keyword}}
