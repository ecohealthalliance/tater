template(name="codingKeywords")
  input.code-search.form-control(type="text" placeholder="Search")
  ul.list-unstyled
    if Template.subscriptionsReady
      each header
        li
          span.code-header(data-id=this._id)= this.header
          ul.code-sub-headings.list-unstyled
            each subHeader this.header
              li
                span.code-sub-header(data-id=this._id)= this.subHeader
                ul.code-keywords.list-unstyled
                  each keywords this.subHeader
                    li
                      span.code-keyword(data-id=this._id)= this.keyword
